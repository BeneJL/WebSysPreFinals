@model StudentPageModel
<!--Page Heading-->
<h1 class="h3 mb-2 text-gray-800">Tables</h1>
<table border="1">
    <thead>
        <tr>
            <th>student_id</th>
            <th>first_name</th>
            <th>last_name</th>
            <th>email</th>
            <th>birth_date</th>
            <th>phone_number</th>
            <th>address</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var row in Model.students){
        <tr>
            <td>student_id</td>
            <td>@row.first_name</td>
            <td>@row.last_name</td>
            <td>@row.email</td>
            <td>@row.birth_date</td>
            <td>@row.phone_number</td>
            <td>@row.address</td>
            </tr>
        }
    </tbody>
</table>